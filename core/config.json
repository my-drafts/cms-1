{
	"db": {
		"enable": true,
		"url": "mongodb://localhost:27017/test",
		"connect": {
			"db": {
				"wtimeout": 5
			},
			"replset": {},
			"mongos": {},
			"server": {
				"autoReconnect": true
			}
		}
	},
	"language": {
		"enable": true,
		"default": "uk",
		"languages": ["en", "uk", "ru"]
	},
	"redirects": [
		{
			"code": 301,
			"replace": "/",
			"matched": "^[\\/]index(?:[\\.](?:html|php|asp))?$"
		}
	]
}